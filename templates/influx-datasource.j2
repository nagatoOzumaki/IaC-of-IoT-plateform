{
  "apiVersion": 1,
  "datasources": [
    {
      "name": "InfluxDB_v2_Flux",
      "type": "influxdb",
      "access": "proxy",
      "url": "http://{{influxdb_container_name}}:8086",
      "jsonData": {
        "version": "Flux",
        "organization": "inpt",
        "defaultBucket": "{{influxdb_init_bucket}}",
        "tlsSkipVerify": true
      },
      "secureJsonData": {
        "token": "{{influxdb_init_admin_token}}"
      }
    }
  ]
}
